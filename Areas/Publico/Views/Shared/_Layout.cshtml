    <script>
        function TestimonialsScroll() {
            let testimonialsNext = document.querySelector(".arrow-next");
            let testimonialsBack = document.querySelector(".arrow-back")

            let scroll = 0;
            let count = 0;

            if (testimonialsNext) {
                testimonialsNext.addEventListener("click", (event) => {
                    count++;
                    scroll += document.querySelector(".testimonial").scrollLeft + 540;
                    document.querySelectorAll(".testimonial-item").forEach((item, index) => {
                        if (count == document.querySelectorAll(".testimonial-item").length) {
                            scroll = 0;
                            count = 0;
                        }
                        item.style = "transform: translateX(-" + scroll.toString() + "px); transition: all 1s ease";
                    });
                    console.log("count: " + count)
                });
            }

            if (testimonialsBack) {
                testimonialsBack.addEventListener("click", (event) => {
                    scroll--;
                    scroll += document.querySelector(".testimonial").scrollLeft - 540;
                    document.querySelectorAll(".testimonial-item").forEach(item => {
                        item.style = "transform: translateX(-" + scroll.toString() + "px); transition: all 1s ease";
                    });

                });
            }
        }
        TestimonialsScroll();
        
    </script>
